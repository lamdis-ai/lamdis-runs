{
  "name": "lamdis-runs",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "build": "tsc -p tsconfig.json",
    "run-file": "tsx src/cli.ts",
    "seed": "tsx scripts/seed.ts",
    "wait": "tsx scripts/wait-for-run.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.938.0",
    "@prisma/client": "^7.2.0",
    "cross-fetch": "^4.0.0",
    "dotenv": "^17.2.3",
    "fastify": "^5.6.2",
    "js-yaml": "^4.1.1",
    "mongoose": "^9.0.2",
    "yaml": "^2.8.2",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@vitest/coverage-v8": "^4.0.16",
    "prisma": "^7.2.0",
    "tsx": "^4.21.0",
    "typescript": "^5.6.3",
    "vitest": "^4.0.16"
  }
}
